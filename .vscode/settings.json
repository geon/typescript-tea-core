{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "json.format.enable": false,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    { "language": "typescript", "autoFix": true },
    { "language": "typescriptreact", "autoFix": true }
  ],
  "search.exclude": {
    "**/node_modules": true,
    "**/dist": true,
    "**/lib": true
  },
  "typescript.tsdk": "node_modules/typescript/lib"
}
